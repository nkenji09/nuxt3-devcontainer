{
  "name": "nuxt",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build --dotenv .env.development",
    "build-staging": "nuxt build --dotenv .env.staging",
    "build-production": "nuxt build --dotenv .env.production",
    "dev": "nuxt dev --dotenv .env.development",
    "dev-staging": "nuxt dev --dotenv .env.staging",
    "dev-production": "nuxt dev --dotenv .env.production",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "precommit": "lint-staged",
    "commit": "git-cz",
    "test": "vitest --watch"
  },
  "dependencies": {
    "neverthrow": "^6.0.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.49.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "happy-dom": "^11.2.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "nuxt": "^3.7.3",
    "nuxt-vitest": "0.10.5",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.3",
    "stylelint": "^15.10.3",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-recommended-scss": "^13.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^5.2.2",
    "vitest": "^0.33.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{ts,tsx,js,vue}": "eslint --fix",
    "./**/*.{css,scss,sass,html,vue}": "stylelint --fix --ignore-path .gitignore",
    "./**/*.{ts,tsx,js,vue,css,scss,sass,html,vue,md,json}": "prettier --write"
  }
}
